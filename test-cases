get product by id
query {
  p1: products(id: "30aTK0HMz4WZWHf2fDZu1iOQFRU") {
    name description price
  }
  p2: products(id: "30aTJzPH7lB8OsfHmkyps6Pww1X") {
    name description price
  }
  p3: products(id: "30aTJwrWCScYY7ERUg1WS3Oqli9") {
    name description price
  }
  p4: products(id: "30aTK7d60o4jMBd50gN5vcMEW7b") {
    name description price
  }
}

see all accounts

query {
  accounts {
    name
    email
    role
    
  }
}

see accounts with pagination
query {
  accounts(pagination: {skip: 3, take:2}) {
    name
    email
    role
    
  }
}

see all products

query {
  products {
    name
    description
    price
  }
}

see products with pagination

query {
  products(pagination: {skip:2, take: 5}) {
    name
    price
    description
  }
}



create orders

mutation {
  o1: createOrder(input: {
    accountId: "30aShnWd0LakpmGCw3hqCeY8jOC"
    products: [
      { id: "30aTK1SERwqTlm3d0ObOLfa8DgV", quantity: 2 },
      { id: "30aTK4d5rmqLO5gvG86Hc3aW86i", quantity: 1 }
    ]
  }) { id totalPrice products { name quantity } }

  o2: createOrder(input: {
    accountId: "30aShsezsy0jdAdA88UqZLqFpvK"
    products: [
      { id: "30aTK3QcsQGmivfJXy6zNljCO7f", quantity: 1 },
