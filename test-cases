signup accounts

mutation {
  a1: signup(input: { name: "Naruto Uzumaki", email: "naruto@leaf.com", password: "shadowclone", role: "user" }) { userId }
  a2: signup(input: { name: "Sasuke Uchiha", email: "sasuke@uchiha.com", password: "chidori", role: "user" }) { userId }
  a3: signup(input: { name: "Sakura Haruno", email: "sakura@leaf.com", password: "healing", role: "user" }) { userId }
  a4: signup(input: { name: "Kakashi Hatake", email: "kakashi@leaf.com", password: "sharingan", role: "user" }) { userId }
  a5: signup(input: { name: "Gaara", email: "gaara@sand.com", password: "sandshield", role: "user" }) { userId }
  a6: signup(input: { name: "Rock Lee", email: "lee@leaf.com", password: "lotus", role: "user" }) { userId }
  a7: signup(input: { name: "Hinata Hyuga", email: "hinata@leaf.com", password: "gentlefist", role: "user" }) { userId }
  a8: signup(input: { name: "Shikamaru Nara", email: "shikamaru@leaf.com", password: "shadowbind", role: "user" }) { userId }
  a9: signup(input: { name: "Itachi Uchiha", email: "itachi@akatsuki.com", password: "amaterasu", role: "user" }) { userId }
  a10: signup(input: { name: "Tsunade", email: "tsunade@leaf.com", password: "slugqueen", role: "admin" }) { userId }
}

login accounts

mutation {
  naruto: login(input: { email: "naruto@leaf.com", password: "shadowclone" }) {
    accessToken refreshToken userId email role
  }

  sasuke: login(input: { email: "sasuke@uchiha.com", password: "chidori" }) {
    accessToken refreshToken userId email role
  }

  sakura: login(input: { email: "sakura@leaf.com", password: "healing" }) {
    accessToken refreshToken userId email role
  }

  kakashi: login(input: { email: "kakashi@leaf.com", password: "sharingan" }) {
    accessToken refreshToken userId email role
  }

  gaara: login(input: { email: "gaara@sand.com", password: "sandshield" }) {
    accessToken refreshToken userId email role
  }

  lee: login(input: { email: "lee@leaf.com", password: "lotus" }) {
    accessToken refreshToken userId email role
  }

  hinata: login(input: { email: "hinata@leaf.com", password: "gentlefist" }) {
    accessToken refreshToken userId email role
  }

  shikamaru: login(input: { email: "shikamaru@leaf.com", password: "shadowbind" }) {
    accessToken refreshToken userId email role
  }

  itachi: login(input: { email: "itachi@akatsuki.com", password: "amaterasu" }) {
    accessToken refreshToken userId email role
  }

  tsunade: login(input: { email: "tsunade@leaf.com", password: "slugqueen" }) {
    accessToken refreshToken userId email role
  }
}

login single account

mutation {
  naruto: login(input: { email: "naruto@leaf.com", password: "shadowclone" }) {
    accessToken refreshToken userId email role
  }
}

