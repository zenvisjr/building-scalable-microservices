search products

query {
  s1: products(query: "kunai") {
    id
    name
    description
    price
  }

  s2: products(query: "chakra") {
    id
    name
    description
    price
  }

  s3: products(query: "cloak") {
    id
    name
    description
    price
  }

  s4: products(query: "scroll") {
    id
    name
    description
    price
  }

  s5: products(query: "eye") {
    id
    name
    description
    price
